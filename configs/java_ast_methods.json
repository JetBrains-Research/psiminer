{
  "filters": [
    {"name": "constructor"},
    {"name": "empty method"},
    {
      "name": "by modifiers",
      "excludeModifiers": ["abstract"]
    },
    {
      "name": "by annotations",
      "excludeAnnotations": ["Override"]
    }
  ],
  "label": {"name": "method name"},
  "storage": {"name": "json tree"},

  "language": "Java",
  "tree transformations": [
    {"name": "resolve type"},
    {"name": "exclude whitespace"},
    {"name": "exclude empty grammar lists"},
    {"name": "exclude language symbols"},
    {
      "name": "hide literals",
      "hideNumbers": true,
      "hideStrings": true
    },
    {
      "name": "remove comments",
      "removeDoc": true
    }
  ],

  "batchSize": 1000,
  "printTrees": false
}
